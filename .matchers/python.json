{
    "problemMatcher": [
        {
            "owner": "pytest",
            "pattern": [
                {
                    "regexp": "^(E)(\\s+)(.*)$",
                    "severity": 1,
                    "message": 3,
                    "code": 3
                },
                {
                    "regexp": "^([\\s->=]*(.*):(\\d*):(\\d*)|.*)$",
                    "file": 2,
                    "line": 3,
                    "column": 4
                }
            ]
        }
    ]
}